#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int arr[]={a, b, c, d};
    int ans= 0;
    for (int i=0; i<=4; i++)
    {
        cout<<endl;
     if( ans<=arr[i])
     {
         ans=arr[i];
     }
     else if (ans> arr[i])
     {
         cout<< ans;
     }
     else {cout<<"input not in correct format
    }
    
   
